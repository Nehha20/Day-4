#include <stdio.h>

void Pattern(int r, int c){
  for(int i=1; i<=r; i++){
    for(int j=1; j<=c ; j++){
      printf("%d", j);
    }
    printf("\n");
  }
}
int main(){
  int val1, val2;
  printf("Enter the no. of rows and col = ");
  scanf("%d %d", &val1, &val2);
  Pattern(val1, val2);
  return 0;
}
