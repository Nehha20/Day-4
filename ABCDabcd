#include <stdio.h>

void Pattern(int r, int c){
  for(int i=0; i<=r; i++){
      for(int j=0; j<=c; j++){
        if(i % 2 == 0){
          printf("%c", 'A'+j);
        }
       else{
        printf("%c", 'a'+j);
       }
    }
    printf("\n");
  }
}
int main(){
  int v1,v2;
  printf("Enter the row & col ");
  scanf("%d %d",&v1, &v2);
  Pattern(v1,v2);
  return 0;
}
