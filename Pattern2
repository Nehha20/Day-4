#include <stdio.h>
void Pattern(int row, int col){
  for(int i=0 ; i<row ; i++){
    for(int j=1 ; j<=col ; j++){
      printf("%d ", j);
    }
    printf("\n");
  }
}
int main(){
  int val1, val2;
  printf("Enter the rows & column : ");
  scanf("%d %d", &val1, &val2);
  Pattern(val1, val2);
  return 0;
}
