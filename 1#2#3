#include <stdio.h>

void Pattern(int r, int c){
  for(int i=0 ; i<=r; i++){
    for(int j=0 ; j<=c ; j++){
      if(j%2 == 0){
        printf("%d ", (j/2) + 1);
      }
      else{
        printf("# ");
      }
    }
    printf("\n");
  }
}
int main(){
  int v1,v2;
  printf("Enter the no.");
  scanf("%d %d",&v1, &v2);
  Pattern(v1,v2);
  return 0;
}
