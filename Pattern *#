#include <stdio.h>
void Pattern(int r, int c){
  for(int i=0 ; i<r ; i++){
    for(int j=0 ; j<c ; j++){
      if(j % 2 == 0){
        printf("* ");
      }
      else{
        printf("# ");
      }
    }
    printf("\n");
  }
}
int main(){
  int val1, val2;
  printf("Enter the row & col ");
  scanf("%d %d", &val1, &val2);
  Pattern(val1, val2);
  return 0;
}
